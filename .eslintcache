out'\n//pages\nimport Homepage from './pages/Homepage';\nimport Registration from './pages/Registration';\nimport Login from './pages/Login';\nimport Recovery from './pages/Recovery';\nimport Dashboard from './pages/Dashboard';\nimport './default.scss';\n\n\nconst App = props => {\n  const dispatch = useDispatch();\n\n  useEffect(() => {\n\n    const authListener = auth.onAuthStateChanged(async userAuth =>{\n       if (userAuth){\n         const userRef = await handleUserProfile(userAuth);\n         userRef.onSnapshot(snapshot => {\n           dispatch(setCurrentUser(\n            {\n              id:snapshot.id,\n              ...snapshot.data()\n            }\n            ))\n         })\n       }\n       dispatch(setCurrentUser(userAuth));\n    });\n    return () => {\n      authListener();\n    }\n}, [])\n\n\n  return (\n    <div className=\"App\">\n     <Switch>\n      <Route exact path=\"/\" render={() =>(\n        <HomepageLayout>\n          <Homepage />\n        </HomepageLayout>\n      )} />\n      <Route path=\"/registration\" render={() => (\n        <MainLayout>\n          <Registration />\n        </MainLayout>\n      )} />\n      <Route path=\"/login\" \n        render={() => (\n          <MainLayout>\n            <Login/>\n          </MainLayout>\n      )} />\n      <Route path=\"/recovery\" render ={() => (\n      <MainLayout>\n        <Recovery />\n      </MainLayout>\n      )} />\n      <Route path=\"/dashboard\" render ={() => (\n      <WithAuth>\n        <MainLayout>\n        <Dashboard />\n      </MainLayout>\n      </WithAuth>\n      )} />\n      </Switch>\n    </div>\n  );\n}\n\n\nexport default App;\n","/home/p/Coding/order/src/components/Header/index.js",["125"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport {useSelector} from 'react-redux';\nimport './styles.scss';\nimport Logo from './../../assets/Logo.png'; \nimport {auth} from './../../firebase/util'\n\nconst mapState= ({user}) => ({\n    currentUser: user.currentUser\n})\n\nconst Header = props => {\n    const {currentUser} = useSelector(mapState);\n    return (\n        <header className=\"header\">\n            <div className=\"wrap\">\n                <div className=\"logo\">\n                    <Link to=\"/\">\n                    <img src={Logo} alt=\"Dhillons\"/>\n                    </Link>\n                </div>\n                <div className=\"callToActions\">\n\n                    {currentUser && (\n                        <ul>\n                            <li>\n                            <Link to=\"/dashboard\">\n                            My Account\n                            </Link>\n                           </li>\n                            <li>\n                                <a onClick ={() => auth.signOut()}>\n                                    LOGOUT\n                                </a>\n                            </li>\n                        </ul>\n                    )}\n                    {!currentUser && (\n                    <ul>\n                        <li>\n                            <Link to=\"/registration\">\n                            sign up\n                            </Link>\n                        </li>\n                        <li>\n                            <Link to=\"/login\">\n                            Login\n                            </Link>\n                        </li>\n                    </ul>\n                )}\n                </div>\n            </div>\n        </header>\n    )\n    \n}\n\nHeader.defaultProps = {\n    currentUser:null\n};\n\n\n\nexport default Header;","/home/p/Coding/order/src/pages/Homepage/index.js",[],"/home/p/Coding/order/src/components/Directory/index.js",["126"],"import React from 'react';\nimport Menu from './../../assets/curry.jpg'\nimport './styles.scss';\n\n\nconst Directory = props => {\n    return (\n        <div className=\"directory\">\n            <div className=\"wrap\">\n            <div\n                className=\"item\"\n                style ={{\n                    backgroundImage: `url(${Menu})`\n                }}\n                >\n                   <a> \n                      View Menu \n                   </a> \n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Directory;","/home/p/Coding/order/src/pages/Registration/index.js",[],"/home/p/Coding/order/src/layouts/MainLayout.js",[],"/home/p/Coding/order/src/layouts/HomepageLayout.js",[],"/home/p/Coding/order/src/components/Footer/index.js",[],"/home/p/Coding/order/src/pages/Login/index.js",[],"/home/p/Coding/order/src/components/SignIn/index.js",["127"],"/home/p/Coding/order/src/components/Forms/Button/index.js",[],"/home/p/Coding/order/src/firebase/util.js",[],"/home/p/Coding/order/src/firebase/config.js",[],"/home/p/Coding/order/src/components/Signup/index.js",["128"],"/home/p/Coding/order/src/components/Forms/FormInput/index.js",[],"/home/p/Coding/order/src/pages/Recovery/index.js",[],"/home/p/Coding/order/src/components/AuthWrapper/index.js",[],"/home/p/Coding/order/src/components/EmailPassword/index.js",["129"],"import React, {useState, useEffect} from 'react';\nimport {useSelector, useDispatch} from 'react-redux';\nimport {withRouter} from 'react-router-dom';\nimport './styles.scss'\nimport {resetPassword, resetAuthForms} from './../../redux/User/user.actions';\nimport AuthWrapper from './../AuthWrapper';\nimport FormInput from './../Forms/FormInput';\nimport Button from './../Forms/Button';\n\n\n\nconst mapState = ({user}) => ({\n    resetPasswordSuccess: user.resetPasswordSuccess,\n    resetPasswordError: user.resetPasswordError\n})\n\nconst EmailPassword = props => {\n    const {resetPasswordSuccess, resetPasswordError} = useSelector(mapState);\n    const dispatch = useDispatch();\n    const [email, setEmail] = useState('');\n    const [errors, setErrors] = useState([]);\n\n    useEffect(() => {\n        if(resetPasswordSuccess) {\n            dispatch(resetAuthForms());\n            props.history.push('/login');\n        }\n    }, [resetPasswordSuccess])\n\n\n    useEffect(() => {\n        if (Array.isArray(resetPasswordError) && resetPasswordError.length > 0) {\n            setErrors(resetPasswordError);\n        }\n    }, [resetPasswordError]);\n\n    \n\n\n   const handleSubmit = e => {\n        e.preventDefault();\n        dispatch(resetPassword({email}));\n        \n    }\n\n    \n       \n\n        const configAuthWrapper = {\n            headline: 'Reset Password with Email'\n        };\n        return (\n            <AuthWrapper {...configAuthWrapper}>\n                <div className=\"formWrap\">\n                    {errors.length > 0 && (\n                        <ul>\n                            {errors.map((e, index) => {\n                                return (\n                                    <li key={index}>\n                                        {e}\n                                    </li>\n                                );\n                            })}\n                        </ul>\n                    )}\n                    <form onSubmit={handleSubmit}>\n\n                        <FormInput\n                        type=\"email\"\n                        name=\"email\"\n                        value={email}\n                        placeholder=\"Your Email\"\n                        handleChange={e => setEmail(e.target.value)}\n                        />\n                        <Button type=\"submit\">Reset Password</Button>\n                    </form>\n                </div>\n            </AuthWrapper>\n        )\n    }\n\n \nexport default withRouter(EmailPassword);","/home/p/Coding/order/src/redux/createStore.js",[],"/home/p/Coding/order/src/redux/rootReducer.js",[],"/home/p/Coding/order/src/redux/User/user.reducer.js",["130","131"],"/home/p/Coding/order/src/redux/User/user.types.js",[],"/home/p/Coding/order/src/redux/User/user.actions.js",[],"/home/p/Coding/order/src/pages/Dashboar