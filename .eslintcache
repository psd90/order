dleUserProfile(user, {displayName});\n            this.setState({\n                ...initialState\n            });\n        }catch(err){\n            console.log(err)\n        }\n    }\n\n    render() {\n        const { displayName, email, password, confirmPassword, errors }  = this.state;\n        const configAuthWrapper = {\n            headline: 'Registration'\n        }\n        return (\n            <AuthWrapper {...configAuthWrapper}>\n                        \n\n                    <div className=\"formWrap\">\n                        {errors.length > 0  && (\n                            <ul>\n                                {errors.map((err, index) => {\n                                    return (\n                                        <li key={index}>\n                                            {err}\n                                        </li>\n                                    )\n                                })}\n                            </ul>\n                        )} \n                    <form onSubmit={this.handleFormSubmit}>\n                        <FormInput \n                        type=\"text\"\n                        name=\"displayName\"\n                        value={displayName}\n                        placeholder=\"Full name\"\n                        onChange ={this.handleChange}\n                        />\n                        <FormInput \n                        type=\"email\"\n                        name=\"email\"\n                        value={email}\n                        placeholder=\"Email\"\n                        onChange ={this.handleChange}\n                        />\n                        <FormInput \n                        type=\"password\"\n                        name=\"password\"\n                        value={password}\n                        placeholder=\"Password\"\n                        onChange ={this.handleChange}\n                        />\n                        <FormInput \n                        type=\"password\"\n                        name=\"confirmPassword\"\n                        value={confirmPassword}\n                        placeholder=\"Confirm Password\"\n                        onChange ={this.handleChange}\n                        />\n                        <Button type=\"submit\">\n                            Register\n                        </Button>\n                    </form>\n                    </div>\n            </AuthWrapper>\n        );\n    }\n}\n\nexport default Signup;","/home/p/Coding/order/src/components/Forms/FormInput/index.js",[],"/home/p/Coding/order/src/pages/Recovery/index.js",[],"/home/p/Coding/order/src/components/AuthWrapper/index.js",[],"/home/p/Coding/order/src/components/EmailPassword/index.js",[],"/home/p/Coding/order/src/redux/createStore.js",[],"/home/p/Coding/order/src/redux/rootReducer.js",[],"/home/p/Coding/order/src/redux/User/user.reducer.js",[],"/home/p/Coding/order/src/redux/User/user.types.js",[],"/home/p/Coding/order/src/redux/User/user.actions.js",[],{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","severity":1,"message":"114","line":23,"column":33,"nodeType":"115","endLine":23,"endColumn":68},{"ruleId":"113","severity":1,"message":"114","line":16,"column":20,"nodeType":"115","endLine":16,"endColumn":23},{"ruleId":"116","severity":1,"message":"117","line":6,"column":33,"nodeType":"118","messageId":"119","endLine":6,"endColumn":44},{"ruleId":"116","severity":1,"message":"120","line":35,"column":63,"nodeType":"118","messageId":"119","endLine":35,"endColumn":69},"no-native-reassign",["121"],"no-negated-in-lhs",["122"],"jsx-a11y/anchor-is-valid","The href attribute is required for an anchor to be keyboard accessible. Provide a valid, navigable address as the href value. If you cannot provide an href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-pl